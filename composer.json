{
  "name": "uploadcare/uploadcare-php",
  "description": "Uploadcare API binding for PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Uploadcare, Inc",
      "email": "hello@uploadcare.com"
    }
  ],
  "keywords": [
    "files",
    "upload",
    "cdn",
    "uploadcare",
    "image management",
    "cloud"
  ],
  "homepage": "https://uploadcare.com",
  "support": {
    "email": "help@uploadcare.com",
    "issues": "https://github.com/uploadcare/uploadcare-php/issues"
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^5.6",
    "ext-curl": "*",
    "ext-json": "*",
    "php-http/client-implementation": "^1.0",
    "php-http/discovery": "^1.0",
    "php-http/httplug": "^1.0",
    "php-http/message-factory": "^1.0",
    "psr/http-message": "^1.0",
    "symfony/polyfill-uuid": "^1.17"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "^5.7",
    "php-http/mock-client": "^1.0",
    "php-http/message": "^1.0",
    "guzzlehttp/psr7": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Uploadcare\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
